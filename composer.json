{
    "name": "devsquad/laravel-extensions",
    "description": "Extension Classes for DevSquad Laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Team DevSquad",
            "email": "team@devsquad.com"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "DevSquad\\Extensions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevSquad\\Extensions\\DevSquadExtensionProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
